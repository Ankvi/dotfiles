/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @background;
    text-color:             @primary;
	font:				 	"Iosevka Nerd Font 10";
}

window {
    width:      			320px;
    padding:    			25px;
    border:					0px 0px 1px 0px;
    border-radius:          0px;
    border-color:           @tertiary;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				@secondary;
}
